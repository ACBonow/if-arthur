{
  "version": 2,
  "buildCommand": "cd InstituicaoFinanceira && npm install",
  "installCommand": "cd InstituicaoFinanceira && npm install",
  "builds": [
    {
      "src": "InstituicaoFinanceira/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/InstituicaoFinanceira/api/index.js"
    }
  ]
}

