{
  "version": 2,
  "builds": [
    {
      "src": "InstituicaoFinanceira/api/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "InstituicaoFinanceira/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/InstituicaoFinanceira/api/index.js"
    }
  ]
}

